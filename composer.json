{
  "name": "solbeg/laravel-vue-validation",
  "description": "Extends Bootstrapper package. It automatically converts Laravel's request rules into Vee's rules.",
  "keywords": ["laravel", "file", "uploader", "file manager", "file uploading"],
  "authors": [
    {
      "name": "Alexey Sejnov",
      "email": "alexey.sejnov@solbeg.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "7.1.*",

    "laravelcollective/html": "^5.4.2",
    "illuminate/support": "~5",
    "illuminate/config": "~5",
    "illuminate/routing": "~5",
    "twbs/bootstrap": "~3",

    "symfony/http-foundation": "*"
  },
  "autoload": {
    "psr-4": { "Solbeg\\VueValidation\\": "src/" }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Solbeg\\VueValidation\\Bootstrapper\\BootstrapperL5ServiceProvider"
      ]
    }
  }
}
